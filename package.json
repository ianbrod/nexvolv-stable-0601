{
  "name": "temp-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "analyze": "npm run build:analyze && npx serve analyze",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:quick": "npm run build && npm run analyze:bundle",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:performance": "vitest run --reporter=verbose src/**/*.performance.test.ts",
    "performance:ci": "node scripts/performance-ci.js",
    "performance:benchmark": "npm run build && npm run test:performance",
    "performance:report": "node -e \"const { PerformanceBenchmark } = require('./src/lib/performance/PerformanceBenchmark'); new PerformanceBenchmark().generateReport().then(console.log)\"",
    "db:reset": "npm run db:clean && npm run db:setup",
    "db:clean": "node scripts/clean-database.js",
    "db:setup": "npx prisma generate && npx prisma db push && npm run db:seed",
    "db:seed": "node prisma/seed.js",
    "db:verify": "node scripts/verify-setup.js",
    "clear-browser-data": "node scripts/clear-browser-data.js",
    "setup": "npm run db:reset",
    "fresh-start": "npm run db:reset && npm run dev",
    "whisper:test": "node scripts/test-whisper-backends.js",
    "whisper:benchmark": "node scripts/benchmark-whisper.js",
    "whisper:install-deps": "node scripts/install-whisper-deps.py"
